package step01.reivew02;

public class Person {
	int age;
	String name;
	String sex;
	String addr;
	
	Person(int age, String name, String sex, String addr){
		this.age=age;
		this.name=name;
		this.sex=sex;
		this.addr=addr;
	}

	public static void main(String[] args) {
		Person p = new Person(11,"Èñ°Ç","³²","¼­¿ï");
		
		System.out.println(p.age);
	}

}
